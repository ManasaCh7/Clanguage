#include<stdio.h>
#include<string.h>
#include<conio.h>
void main()
{
  int a[10][10];
  int b;
  printf("enter num of names:");
  scanf("%d",&b);
  printf("enter all names:");
  for(int i=0;i<b;i++)
  {
   scanf("%s",a[i]);
  }
  printf("the lengends are: ");
  for(int i=0;i<b;i++)
  {
    printf("no %d%s",i+1,a[i]);
  }
}
